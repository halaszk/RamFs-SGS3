#!/sbin/busybox sh

if [ "a$2" != "a" ];then

filename=$(basename $2)

if [ "a$3" == "a" ];then
  evalcmd='eval "echo \$$filename"'
  eval $evalcmd
  exit
else
  evalcmd='eval "$filename=$3"'
  eval $evalcmd
  evalcmd='eval "echo \$$filename"'
  eval $evalcmd
fi

  if [ -f /sys/devices/system/cpu/cpu0/cpufreq/UV_mV_table ];then
 case "$cpu_undervolting" in
  0)
        echo "1400 1400 1375 1313 1275 1238 1175 1125 1075 1025 975 963 950 938 925 900 900" > /sys/devices/system/cpu/cpu0/cpufreq/UV_mV_table
    ;;
  25)
        echo "1375 1375 1350 1288 1250 1213 1150 1100 1050 1000 950 938 925 913 900 875 875" > /sys/devices/system/cpu/cpu0/cpufreq/UV_mV_table
    ;;
  50)
        echo "1350 1350 1325 1263 1225 1188 1125 1075 1025 975 925 913 900 888 875 850 850" > /sys/devices/system/cpu/cpu0/cpufreq/UV_mV_table
    ;;
  75)
        echo "1325 1325 1300 1238 1200 1163 1100 1050 1000 950 900 888 875 863 850 825 825" > /sys/devices/system/cpu/cpu0/cpufreq/UV_mV_table
    ;;
  100)
        echo "1300 1300 1275 1213 1175 1138 1075 1025 975 925 875 863 850 838 825 800 800" > /sys/devices/system/cpu/cpu0/cpufreq/UV_mV_table
    ;;
esac;
fi
fi
